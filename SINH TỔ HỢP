#include <bits/stdc++.h>

using namespace std;

int n, k, a[100], kt;
void rise(){
    for(int i = 1; i <= k; ++i) a[i] = i;
}
void sinh(){
    int i = k;
    while(i >= 1 && a[i] == n - k + i){
        --i;
    }
    if(i == 0) kt=0;
    else{
        a[i]++;
        for(int j = i + 1; j <= k; ++j){
            a[j] = a[j-1] + 1;
        }
    }
}

int main()
{

    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int t;
    cin >>t;
    while(t--){
        cin >>n >>k;
        kt=1;
        rise();
        while(kt){
                for(int i = 1; i <= k; ++i) cout <<a[i];
                cout <<" ";
            sinh();
        }
        cout <<'\n';
    }
   return 0;
}
