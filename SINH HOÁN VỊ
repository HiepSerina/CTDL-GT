#include <bits/stdc++.h>

using namespace std;

int main()
{

    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int t;
    cin >>t;
    while(t--){
        int n;
        cin >>n;
        int a[n+1];
        for(int i = 0; i < n; ++i) a[i] = i + 1;
        do{
            for(int i = 0; i < n; ++i){
                cout <<a[i];
            }
            cout <<" ";
        }while(next_permutation(a,a+n));
        cout <<'\n';
    }
   return 0;
}
