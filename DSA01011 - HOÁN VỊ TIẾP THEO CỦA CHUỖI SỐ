#include <bits/stdc++.h>

using namespace std;

int main()
{

    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    //freopen("input.txt","r",stdin);
    //freopen("output.txt","w",stdout);
    int t;
    cin >>t;
    while(t--){
        string s;
        int n;
        cin >>n >>s;
        cout <<n <<" ";
        if(next_permutation(s.begin(),s.end())){
            cout <<s;
        }
        else cout <<"BIGGEST";
        cout <<'\n';
    }
   return 0;
}
